<Window x:Class="SharpGit.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SharpGit"
        mc:Ignorable="d"
        Title="MainWindow" Height="345.098" Width="531.863">
    <Grid>
        <TextBox x:Name="UrlText" Height="21" Margin="53,15,10,0" TextWrapping="Wrap" Text="https://github.com/Kidel/Plotter.git" VerticalAlignment="Top"/>
        <TextBox x:Name="PathText" Height="21" Margin="53,41,10,0" TextWrapping="Wrap" Text="D:\GitHub\test" VerticalAlignment="Top"/>
        <Label Content="Url" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Path" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top"/>
        <Button x:Name="CloneButton" Content="Clone" HorizontalAlignment="Left" Margin="53,67,0,0" VerticalAlignment="Top" Width="75" Click="Clone"/>
        <Button x:Name="PullButton" Content="Pull" HorizontalAlignment="Left" Margin="133,67,0,0" VerticalAlignment="Top" Width="75" Click="Pull"/>
        <TextBlock x:Name="Output" Margin="10,102,10,10" TextWrapping="Wrap" Text=""/>

    </Grid>
</Window>
